{
  "name": "@tugabridge/subgraph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create tugabridge/@tugabridge/subgraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create tugabridge/@tugabridge/subgraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph codegen && graph build",
    "deploy": "graph deploy tugabridge/@tugabridge/subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy tugabridge/@tugabridge/subgraph --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.30.2",
    "@graphprotocol/graph-ts": "^0.27.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}